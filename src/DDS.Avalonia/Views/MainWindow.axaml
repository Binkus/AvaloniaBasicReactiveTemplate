<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:DDS.Core.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:DDS.Avalonia.Views"
        xmlns:reactiveUi="http://reactiveui.net"
        xmlns:viewModels="clr-namespace:DDS.Core.ViewModels;assembly=DDS.Core"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="DDS.Avalonia.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="DDS"
        WindowStartupLocation="CenterScreen"
        Content="{Binding MainView}">
    <Design.DataContext>
        <viewModels:MainWindowViewModel />
    </Design.DataContext>
</Window>
